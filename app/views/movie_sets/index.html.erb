<div class="container">
  <div class="main">
  <h2>SETS OF YOUR DREAM</h1>
    <div class="cards">
      <% @movie_sets.each do |movie_set| %>
      <div class="card-trip">
          <%= link_to movie_set_path(movie_set) do %>
          <img class="card-trip-img" src="https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/greece.jpg" />
          <% end %>
        <div class="card-trip-infos">
          <div>
            <h2><%= movie_set.title %></h2>
            <p><%= movie_set.address %></p>
          </div>
          <% end %>
        </div>
    </div>
  </div>
</div>
